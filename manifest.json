{
  "name": "Time Blocking Calendar",
  "description": "Syncs with your Google Calendar to improve your productivity using the Time Blocking time management technique.",
  "version": "1.0",
  "manifest_version": 3,
  "key": "",
  "oauth2": {
    "client_id": "546569437349-5025l84ori8se7m9kl37hvdo3vbqiulm.apps.googleusercontent.com",
    "scopes": [
      "openid", 
      "email", 
      "profile",
      "https://www.googleapis.com/auth/calendar"
    ]
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "identity",
    "identity.email"
  ],
  "host_permissions": ["https://www.googleapis.com/*"],
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens popup.html"
    }
  }
  }